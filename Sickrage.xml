<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Dux SickRage</Name>
  <Description>Sickrage.
</Description>
  <Registry>https://registry.hub.docker.com/u/duxducis/sickrage/</Registry>
  <Repository>duxducis/sickrage</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
     <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>8081</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>Config directory on unRaid</HostDir>
      <ContainerDir>/config</ContainerDir>
    </Volume>
  </Data>
  <Version>44eceaa0</Version>
  <WebUI>http://[IP]:[PORT:8081]/</WebUI>
  <Banner>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/sickrage-banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/sickrage-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
